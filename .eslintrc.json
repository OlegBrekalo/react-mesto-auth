{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-underscore-dangle": ["off", { "allow": ["_id"] }],
    "react/jsx-wrap-multilines": ["error", { "declaration": false, "assignment": false }],
    "no-console": ["off"]
  }
}
